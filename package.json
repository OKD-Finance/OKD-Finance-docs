{
  "name": "okd-finance-docs",
  "version": "1.0.0",
  "description": "OKD Finance Exchange API Documentation",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "preview": "vitepress preview docs",
    "lint": "eslint . --ext .vue,.js,.ts",
    "lint:fix": "eslint . --ext .vue,.js,.ts --fix",
    "prepare": "husky",
    "generate-api-docs": "node swagger-generator.cjs",
    "update-api-docs": "node update-api-docs.cjs",
    "build-docs": "npm run generate-api-docs && npm run update-api-docs && npm run build",
    "dev-docs": "npm run generate-api-docs && npm run update-api-docs && npm run dev"
  },
  "keywords": [
    "crypto",
    "exchange",
    "trading",
    "bybit",
    "api",
    "documentation"
  ],
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "mermaid": "^11.6.0",
    "vitepress": "^1.6.3",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vue": "^3.5.16"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "vite": {
      "esbuild": "^0.25.0"
    },
    "vitepress": {
      "vite": {
        "esbuild": "^0.25.0"
      }
    }
  },
  "lint-staged": {
    "*.{vue,js,ts}": [
      "eslint --fix"
    ]
  }
}
